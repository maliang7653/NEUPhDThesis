%调整期刊年份的格式，去掉括号
\newbibmacro*{issue+date}{%
  \printtext{%去掉了[parens]
    \iffieldundef{issue}
      {%\usebibmacro{date}
        \iffieldundef{note}{\usebibmacro{date}}%判断一下，是否是报纸和标准
        {\def\comparetmp{news}\edef\comparetmpb{\thefield{note}}%
        \ifx\comparetmp\comparetmpb%
        \usebibmacro{newsdate}%
        \else%
        \usebibmacro{date}%
        \fi}%
      }
      {\printfield{issue}%
       \setunit*{\addspace}%
       %\usebibmacro{date}
         \iffieldundef{note}{\usebibmacro{date}}%判断一下，是否是报纸和标准
        {\def\comparetmp{news}\edef\comparetmpb{\thefield{note}}%
        \ifx\comparetmp\comparetmpb%
        \usebibmacro{newsdate}%
        \else%
        \usebibmacro{date}%
        \fi}%
       }%
       }%
  %\newunit
  }

